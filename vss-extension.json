{
    "manifestVersion": 1,
    "id": "Test-Analisys",
    "version": "1.0.3",
    "name": "Solution to Analizes your Tests",
    "description": "A sample Visual Studio Services extension.",
    "publisher": "AviHadad",
    "categories": [
        "Test"
    ],
    "targets": [{
        "id": "Microsoft.VisualStudio.Services",
        "version": "[15.0,)"
    }],
    "icons": {
        "default": "images/logo.png"
    },
    "demands": ["contribution/ms.vss-dashboards-web.widget-sdk-version-2", "contribution/ms.vss-web.charts-service"],
    "contributions": [{
        "id": "Test_Analisys",
        "type": "ms.vss-web.hub",
        "description": "Adds a 'Analisys' hub to the test hub group.",
        "targets": [
            "ms.vss-test-web.test-hub-group"
        ],
        "properties": {
            "name": "Analisys",
            "order": 99,
            "uri": "Test-Analisys.html"
        }
    }],
    "scopes": [
        "vso.work",
        "vso.loadtest_write",
        "vso.test",
        "vso.test_write",
        "vso.loadtest"
    ],
    "files": [{
            "path": "Test-Analisys.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "scripts",
            "addressable": true
        },
        {
            "path": "images/logo.png",
            "addressable": true
        }
    ]
}